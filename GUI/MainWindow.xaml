<Window x:Class="GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        mc:Ignorable="d"
        Title="MainWindow" 
        Height="700" 
        Width="980"
        Background="#242424"
        WindowStyle="None"
        ResizeMode="CanResize"
        WindowStartupLocation="CenterScreen" 
        IsEnabled="False"
        MouseDown="Window_MouseDown">

    <Window.Resources>


        <!--// ANIMACIÓN PARA MOSTRAR MAPA //-->
        <Storyboard x:Key="ShowMapAnimation">
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="MapView_UC"
                           Storyboard.TargetProperty="Visibility"
                           BeginTime="0:0:0">
                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{x:Static Visibility.Visible}"/>
            </ObjectAnimationUsingKeyFrames>

            <DoubleAnimation Storyboard.TargetName="MapView_UC"
             Storyboard.TargetProperty="Opacity"
             From="0" To="1" 
             BeginTime="0:0:0" Duration="0:0:0.5">
                <DoubleAnimation.EasingFunction>
                    <CircleEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>

            <DoubleAnimation Storyboard.TargetName="MapView_UC"
             Storyboard.TargetProperty="(UIElement.RenderTransform).(TranslateTransform.Y)"
             From="20" To="0" 
             BeginTime="0:0:0" Duration="0:0:0.5">
                <DoubleAnimation.EasingFunction>
                    <CircleEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
        </Storyboard>

        <!--// ANIMACIÓN PARA OCULTAR MAPA //-->
        <Storyboard x:Key="HideMapAnimation">
            <DoubleAnimation Storyboard.TargetName="MapView_UC"
             Storyboard.TargetProperty="Opacity"
             From="1" To="0" 
             Duration="0:0:0.3">
            </DoubleAnimation>

            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="MapView_UC"
                           Storyboard.TargetProperty="Visibility"
                           BeginTime="0:0:0.3">
                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{x:Static Visibility.Collapsed}"/>
            </ObjectAnimationUsingKeyFrames>
        </Storyboard>








        <!--// SubmenuOne Vertical Slide-In Animation //-->
        <Storyboard x:Key="SubmenuOne_Enter">
            <DoubleAnimation Storyboard.Target="{Binding ElementName=SubmenuOne_UC}"
                             Storyboard.TargetProperty="(RenderTransform).(TranslateTransform.Y)"
                             From="{Binding ElementName=SubmenuOne_Container, Path=ActualHeight}"
                             Duration="0:0:0.8"
                             AccelerationRatio="0.9"
                             FillBehavior="HoldEnd">
                <DoubleAnimation.EasingFunction>
                    <CircleEase EasingMode="EaseOut" />
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>

            <DoubleAnimation Storyboard.Target="{Binding ElementName=SubmenuOne_UC}"
                             Storyboard.TargetProperty="Opacity"
                             From="0"
                             To="1" />

            <ObjectAnimationUsingKeyFrames BeginTime="00:00:00"
                                           Storyboard.Target="{Binding ElementName=SubmenuOne_UC}"
                                           Storyboard.TargetProperty="(UIElement.Visibility)">
                <DiscreteObjectKeyFrame KeyTime="00:00:00"
                                        Value="{x:Static Visibility.Visible}" />
            </ObjectAnimationUsingKeyFrames>
        </Storyboard>

        <!--// SubmenuOne Vertical Slide-Out Animation //-->
        <Storyboard x:Key="SubmenuOne_Exit">
            <DoubleAnimation Storyboard.Target="{Binding ElementName=SubmenuOne_UC}"
                             Storyboard.TargetProperty="(RenderTransform).(TranslateTransform.Y)"
                             To="-650"
                             Duration="0:0:1.5"
                             AccelerationRatio="0.9"
                             FillBehavior="HoldEnd">
                <DoubleAnimation.EasingFunction>
                    <CircleEase EasingMode="EaseOut" />
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>

            <DoubleAnimation Storyboard.Target="{Binding ElementName=SubmenuOne_UC}"
                             Storyboard.TargetProperty="Opacity"
                             From="1"
                             To="0" />
        </Storyboard>

        <!--// SubmenuTwo Horizontal Slide-In Animation //-->
        <Storyboard x:Key="SubmenuTwo_Enter">
            <DoubleAnimation Storyboard.Target="{Binding ElementName=SubmenuTwo_UC}"
                             Storyboard.TargetProperty="(RenderTransform).(TranslateTransform.X)"
                             From="{Binding ElementName=SubmenuTwo_Container, Path=ActualWidth}"
                             Duration="0:0:1"
                             FillBehavior="HoldEnd">
                <DoubleAnimation.EasingFunction>
                    <CircleEase EasingMode="EaseOut" />
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>

            <DoubleAnimation Storyboard.Target="{Binding ElementName=SubmenuTwo_UC}"
                             Storyboard.TargetProperty="Opacity"
                             From="0"
                             To="1" />

            <ObjectAnimationUsingKeyFrames BeginTime="00:00:00"
                                           Storyboard.Target="{Binding ElementName=SubmenuTwo_UC}"
                                           Storyboard.TargetProperty="(UIElement.Visibility)">
                <DiscreteObjectKeyFrame KeyTime="00:00:00"
                                        Value="{x:Static Visibility.Visible}" />
            </ObjectAnimationUsingKeyFrames>
        </Storyboard>

        <!--// SubmenuTwo Horizontal Slide-Out Animation //-->
        <Storyboard x:Key="SubmenuTwo_Exit">
            <DoubleAnimation Storyboard.Target="{Binding ElementName=SubmenuTwo_UC}"
                             Storyboard.TargetProperty="(RenderTransform).(TranslateTransform.X)"
                             To="{Binding ElementName=SubmenuTwo_Container, Path=ActualWidth}"
                             Duration="0:0:1"
                             AccelerationRatio="0.4"
                             FillBehavior="HoldEnd" />

            <DoubleAnimation Storyboard.Target="{Binding ElementName=SubmenuTwo_UC}"
                             Storyboard.TargetProperty="Opacity"
                             From="1"
                             To="0" />

            <ObjectAnimationUsingKeyFrames BeginTime="00:00:1"
                                           Storyboard.Target="{Binding ElementName=SubmenuTwo_UC}"
                                           Storyboard.TargetProperty="(UIElement.Visibility)">
                <DiscreteObjectKeyFrame KeyTime="00:00:1"
                                        Value="{x:Static Visibility.Collapsed}" />
            </ObjectAnimationUsingKeyFrames>
        </Storyboard>

        <!--// SOLO ANIMACIÓN DEL LOGO - SIN CHAT AUTOMÁTICO //-->
        <Storyboard x:Key="LogoAnimation">
            <!-- Mostrar logo -->
            <ObjectAnimationUsingKeyFrames BeginTime="0:0:3.8"
                                   Storyboard.TargetName="Logo"
                                   Storyboard.TargetProperty="(UIElement.Visibility)">
                <DiscreteObjectKeyFrame KeyTime="00:00:00"
                                Value="{x:Static Visibility.Visible}" />
            </ObjectAnimationUsingKeyFrames>

            <!-- Fade in del logo -->
            <DoubleAnimation Storyboard.TargetName="Logo"
                     Storyboard.TargetProperty="Opacity"
                     From="0"
                     To="1"
                     BeginTime="0:0:3.8"
                     Duration="0:0:2"
                     RepeatBehavior="1x"
                     AutoReverse="True" 
                     Completed="EnableMainWindow">
                <DoubleAnimation.EasingFunction>
                    <CircleEase EasingMode="EaseOut" />
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
        </Storyboard>

        <!--// ANIMACIÓN PARA MOSTRAR CHAT //-->
        <Storyboard x:Key="ShowChatAnimation">
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Chat_UC"
                                   Storyboard.TargetProperty="Visibility"
                                   BeginTime="0:0:0">
                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{x:Static Visibility.Visible}"/>
            </ObjectAnimationUsingKeyFrames>

            <DoubleAnimation Storyboard.TargetName="Chat_UC"
                     Storyboard.TargetProperty="Opacity"
                     From="0" To="1" 
                     BeginTime="0:0:0" Duration="0:0:0.5">
                <DoubleAnimation.EasingFunction>
                    <CircleEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>

            <DoubleAnimation Storyboard.TargetName="Chat_UC"
                     Storyboard.TargetProperty="(UIElement.RenderTransform).(TranslateTransform.Y)"
                     From="20" To="0" 
                     BeginTime="0:0:0" Duration="0:0:0.5">
                <DoubleAnimation.EasingFunction>
                    <CircleEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
        </Storyboard>

        <!--// ANIMACIÓN PARA OCULTAR CHAT //-->
        <Storyboard x:Key="HideChatAnimation">
            <DoubleAnimation Storyboard.TargetName="Chat_UC"
                     Storyboard.TargetProperty="Opacity"
                     From="1" To="0" 
                     Duration="0:0:0.3">
            </DoubleAnimation>

            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Chat_UC"
                                   Storyboard.TargetProperty="Visibility"
                                   BeginTime="0:0:0.3">
                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{x:Static Visibility.Collapsed}"/>
            </ObjectAnimationUsingKeyFrames>
        </Storyboard>

        <!--// ANIMACIÓN PARA MOSTRAR HISTORIAL //-->
        <Storyboard x:Key="ShowHistorialAnimation">
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Historial_UC"
                                   Storyboard.TargetProperty="Visibility"
                                   BeginTime="0:0:0">
                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{x:Static Visibility.Visible}"/>
            </ObjectAnimationUsingKeyFrames>

            <DoubleAnimation Storyboard.TargetName="Historial_UC"
                     Storyboard.TargetProperty="Opacity"
                     From="0" To="1" 
                     BeginTime="0:0:0" Duration="0:0:0.5">
                <DoubleAnimation.EasingFunction>
                    <CircleEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>

            <DoubleAnimation Storyboard.TargetName="Historial_UC"
                     Storyboard.TargetProperty="(UIElement.RenderTransform).(TranslateTransform.Y)"
                     From="20" To="0" 
                     BeginTime="0:0:0" Duration="0:0:0.5">
                <DoubleAnimation.EasingFunction>
                    <CircleEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
        </Storyboard>

        <!--// ANIMACIÓN PARA OCULTAR HISTORIAL //-->
        <Storyboard x:Key="HideHistorialAnimation">
            <DoubleAnimation Storyboard.TargetName="Historial_UC"
                     Storyboard.TargetProperty="Opacity"
                     From="1" To="0" 
                     Duration="0:0:0.3">
            </DoubleAnimation>

            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Historial_UC"
                                   Storyboard.TargetProperty="Visibility"
                                   BeginTime="0:0:0.3">
                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{x:Static Visibility.Collapsed}"/>
            </ObjectAnimationUsingKeyFrames>
        </Storyboard>




        <!--// ANIMACIÓN PARA MOSTRAR LOGIN //-->
        <Storyboard x:Key="ShowLoginAnimation">
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Login_UC"
                           Storyboard.TargetProperty="Visibility"
                           BeginTime="0:0:0">
                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{x:Static Visibility.Visible}"/>
            </ObjectAnimationUsingKeyFrames>

            <DoubleAnimation Storyboard.TargetName="Login_UC"
             Storyboard.TargetProperty="Opacity"
             From="0" To="1" 
             BeginTime="0:0:0" Duration="0:0:0.5">
                <DoubleAnimation.EasingFunction>
                    <CircleEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>

            <DoubleAnimation Storyboard.TargetName="Login_UC"
             Storyboard.TargetProperty="(UIElement.RenderTransform).(TranslateTransform.Y)"
             From="20" To="0" 
             BeginTime="0:0:0" Duration="0:0:0.5">
                <DoubleAnimation.EasingFunction>
                    <CircleEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
        </Storyboard>

        <!--// ANIMACIÓN PARA OCULTAR LOGIN //-->
        <Storyboard x:Key="HideLoginAnimation">
            <DoubleAnimation Storyboard.TargetName="Login_UC"
             Storyboard.TargetProperty="Opacity"
             From="1" To="0" 
             Duration="0:0:0.3">
            </DoubleAnimation>

            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Login_UC"
                           Storyboard.TargetProperty="Visibility"
                           BeginTime="0:0:0.3">
                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{x:Static Visibility.Collapsed}"/>
            </ObjectAnimationUsingKeyFrames>
        </Storyboard>

        <!--// ANIMACIÓN PARA MOSTRAR REGISTER //-->
        <Storyboard x:Key="ShowRegisterAnimation">
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Register_UC"
                           Storyboard.TargetProperty="Visibility"
                           BeginTime="0:0:0">
                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{x:Static Visibility.Visible}"/>
            </ObjectAnimationUsingKeyFrames>

            <DoubleAnimation Storyboard.TargetName="Register_UC"
             Storyboard.TargetProperty="Opacity"
             From="0" To="1" 
             BeginTime="0:0:0" Duration="0:0:0.5">
                <DoubleAnimation.EasingFunction>
                    <CircleEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
        </Storyboard>

        <!--// ANIMACIÓN PARA OCULTAR REGISTER //-->
        <Storyboard x:Key="HideRegisterAnimation">
            <DoubleAnimation Storyboard.TargetName="Register_UC"
             Storyboard.TargetProperty="Opacity"
             From="1" To="0" 
             Duration="0:0:0.3">
            </DoubleAnimation>

            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Register_UC"
                           Storyboard.TargetProperty="Visibility"
                           BeginTime="0:0:0.3">
                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{x:Static Visibility.Collapsed}"/>
            </ObjectAnimationUsingKeyFrames>
        </Storyboard>

        <!--// ANIMACIÓN PARA MOSTRAR FORGOT PASSWORD //-->
        <Storyboard x:Key="ShowForgotPasswordAnimation">
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ForgotPassword_UC"
                           Storyboard.TargetProperty="Visibility"
                           BeginTime="0:0:0">
                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{x:Static Visibility.Visible}"/>
            </ObjectAnimationUsingKeyFrames>

            <DoubleAnimation Storyboard.TargetName="ForgotPassword_UC"
             Storyboard.TargetProperty="Opacity"
             From="0" To="1" 
             BeginTime="0:0:0" Duration="0:0:0.5">
                <DoubleAnimation.EasingFunction>
                    <CircleEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
        </Storyboard>

        <!--// ANIMACIÓN PARA OCULTAR FORGOT PASSWORD //-->
        <Storyboard x:Key="HideForgotPasswordAnimation">
            <DoubleAnimation Storyboard.TargetName="ForgotPassword_UC"
             Storyboard.TargetProperty="Opacity"
             From="1" To="0" 
             Duration="0:0:0.3">
            </DoubleAnimation>

            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ForgotPassword_UC"
                           Storyboard.TargetProperty="Visibility"
                           BeginTime="0:0:0.3">
                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{x:Static Visibility.Collapsed}"/>
            </ObjectAnimationUsingKeyFrames>
        </Storyboard>




    </Window.Resources>






    <Grid x:Name = "Manin_windonw_Panel"
          Background="Transparent" 
          MouseDown="Manin_windonw_Panel_MouseDown">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="977*"/>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition Width="3*"/>
        </Grid.ColumnDefinitions>

        <!--// Background Border //-->
        <Border CornerRadius="10"                    
        BorderThickness="2"
        Opacity="0.95"
        Panel.ZIndex="0">

            <Border.BorderBrush>
                <LinearGradientBrush StartPoint="0,0" EndPoint="1,1">
                    <GradientStop Color="#0066CC" Offset="0"/>
                    <GradientStop Color="#00A3E0" Offset="0.5"/>
                    <GradientStop Color="#005EB8" Offset="1"/>
                </LinearGradientBrush>
            </Border.BorderBrush>

            <Border.Background>
                <LinearGradientBrush StartPoint="0,1" EndPoint="1,0">
                    <GradientStop Color="#FFFFFF" Offset="0"/>
                    <GradientStop Color="#E6F2FF" Offset="0.5"/>
                    <GradientStop Color="#CCE5FF" Offset="1"/>
                </LinearGradientBrush>
            </Border.Background>

            <Image x:Name="Logo"
           Source="{StaticResource Logo_1}"
           Visibility="Collapsed"
           Stretch="None"
           HorizontalAlignment="Center"
           VerticalAlignment="Center"
           Margin="0,0,10,20">

                <Image.Triggers>
                    <EventTrigger RoutedEvent="Image.Loaded">
                        <BeginStoryboard Storyboard="{StaticResource LogoAnimation}" />
                    </EventTrigger>
                </Image.Triggers>

            </Image>

        </Border>
        <!--//Add chatview View //-->
        <local:ChatView x:Name="Chat_UC"
        Panel.ZIndex="10"
        Grid.ColumnSpan="3"
        HorizontalAlignment="Stretch"
        VerticalAlignment="Stretch"
        MinWidth="500"
        MinHeight="400"
        Margin="240,90,30,30"
        Visibility="Collapsed" 
        Opacity="0">
            <local:ChatView.RenderTransform>
                <TranslateTransform Y="0"/>
            </local:ChatView.RenderTransform>
        </local:ChatView>

        <!-- // Add Historial View // -->
        <local:Historial x:Name="Historial_UC"
            Panel.ZIndex="9"
            Grid.ColumnSpan="3"
            HorizontalAlignment="Stretch"
            VerticalAlignment="Stretch"
            MinWidth="500"
            MinHeight="400"
            Margin="240,90,30,30"
            Visibility="Collapsed"
            Opacity="0">
            <local:Historial.RenderTransform>
                <TranslateTransform Y="0"/>
            </local:Historial.RenderTransform>
        </local:Historial>


        <!--//Add Mapa View //-->
        <local:MapView x:Name="MapView_UC"
            Panel.ZIndex="9"
            Grid.ColumnSpan="3"
            HorizontalAlignment="Stretch"
            VerticalAlignment="Stretch"
            MinWidth="500"
            MinHeight="400"
            Margin="240,90,30,30"
            Visibility="Collapsed" 
            Opacity="0">
            <local:MapView.RenderTransform>
                <TranslateTransform Y="0"/>
            </local:MapView.RenderTransform>
        </local:MapView>

        <!--//Add Login View //-->
        <local:LoginUserControl x:Name="Login_UC"
                Panel.ZIndex="10"
                Grid.ColumnSpan="3"
                VerticalAlignment="Center" 
                HorizontalAlignment="Center"     
                Margin="0,40,0,0"
                Visibility="Collapsed" 
                Opacity="0">
            <local:LoginUserControl.RenderTransform>
                <TranslateTransform Y="0"/>
            </local:LoginUserControl.RenderTransform>
        </local:LoginUserControl>

        <!--//Add Register View //-->
        <local:RegisterUserControl x:Name="Register_UC"
                Panel.ZIndex="10"
                Grid.ColumnSpan="3"
                VerticalAlignment="Center" 
                HorizontalAlignment="Center"
                Margin="0,0,0,0"
                Visibility="Collapsed" 
                Opacity="0">
            <local:RegisterUserControl.RenderTransform>
                <TranslateTransform Y="40"/>
            </local:RegisterUserControl.RenderTransform>
        </local:RegisterUserControl>


        <!--//Add ForgotPassword View //-->
        <local:ForgotPasswordUserControl x:Name="ForgotPassword_UC"
                Panel.ZIndex="10"
                Grid.ColumnSpan="3"
                VerticalAlignment="Center" 
                HorizontalAlignment="Center"
                Margin="0,-40,0,0"
                Visibility="Collapsed" 
                Opacity="0">
            <local:ForgotPasswordUserControl.RenderTransform>
                <TranslateTransform Y="40"/>
            </local:ForgotPasswordUserControl.RenderTransform>
        </local:ForgotPasswordUserControl>





        <!--// Add MainMenu UserControl //-->
        <local:MainMenu x:Name="MainMenu_UC"
                HorizontalAlignment="Left"
                VerticalAlignment="Center"
                Width="980"
                Height="700"
                Panel.ZIndex="5"
                Background="Transparent" Grid.ColumnSpan="3" Loaded="MainMenu_UC_Loaded"
                />



        <!--// Add SubmenuOne UserControl //-->
        <Canvas x:Name="SubmenuOne_Container"
        Margin="292,0,0,0"
        Background="Transparent"
                Panel.ZIndex="6">

            <local:SubmenuOne x:Name="SubmenuOne_UC"
                      Visibility="Collapsed"
                      Panel.ZIndex="0">

                <local:SubmenuOne.RenderTransform>
                    <TranslateTransform X="0"
                                Y="0" />
                </local:SubmenuOne.RenderTransform>

            </local:SubmenuOne>

        </Canvas>

        <!--// Add SubmenuTwo UserControl //-->
        <Canvas x:Name="SubmenuTwo_Container"
       Margin="530,100,0,0"
       Panel.ZIndex="7"
       Background="Transparent">
            <local:SubmenuTwo x:Name="SubmenuTwo_UC"
                     Visibility="Collapsed"
                     Panel.ZIndex="10"
                     RenderTransformOrigin="0,0">
                <local:SubmenuTwo.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform ScaleX="1.0" ScaleY="1.0"/>
                        <TranslateTransform X="0" Y="0" />
                    </TransformGroup>
                </local:SubmenuTwo.RenderTransform>
            </local:SubmenuTwo>
        </Canvas>



        <!--// TOP, Botton Border Desing//-->
        <Border x:Name="Window_Edge"
         BorderBrush="#FFFFFF"
         BorderThickness=" 0,4,0,4"
         Panel.ZIndex="1" Grid.ColumnSpan="3" />

        <!--// Add AppControl UserControl - Ajustado 1cm más arriba //-->
        <local:AppControl Panel.ZIndex="8" Margin="0,-5,0,5" />

    </Grid>

</Window>